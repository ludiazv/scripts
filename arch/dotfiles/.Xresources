!URxvt.font: xft:Terminus:pixelsize=10
urxvt*font:             xft:Dejavu Sans Mono:size=12
urxvt*termName:         rxvt
urxvt*background:       #000000
urxvt*foreground:       #cccccc
urxvt*scrollBar:        false
urxvt*perl-lib:         /usr/lib/urxvt/perl/
!urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      /usr/bin/jumanji
urxvt*matcher.button:   1
urxvt*fading:           20
urxvt*fadeColor:        #000000

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

urxvt*background:       rgba:0000/0000/0000/cccc

URxvt.transparent:   true
URxvt.tintColor:     white
URxvt.shading:       20 
 
URxvt*color0:      #000000
URxvt*color1:      #9e1828
URxvt*color2:      #aece92
URxvt*color3:      #968a38
URxvt*color4:      #414171
URxvt*color5:      #963c59
URxvt*color6:      #418179
URxvt*color7:      #bebebe
URxvt*color8:      #666666
URxvt*color9:      #cf6171
URxvt*color10:     #c5f779
URxvt*color11:     #fff796
URxvt*color12:     #4186be
URxvt*color13:     #cf9ebe
URxvt*color14:     #71bebe
URxvt*color15:     #ffffff


URxvt.perl-ext-common: matcher,selection-to-clipboard
!URxvt.keysym.M-Control-v: eval:paste_clipboard
!URxvt.keysym.M-c:   perl:clipboard:copy
!URxvt.keysym.M-v:   perl:clipboard:paste
!URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.clipboard.autocopy:   true


!Rofi configs
!------------------
! Disabled -> passed to themes
!rofi.fullscreen: true
!rofi.lines: 8
!rofi.padding: 150
!rofi.hide-scrollbar: true
!rofi.font: Hack 18



!rofi.color-enabled: true
!rofi.color-window: argb:cc2f343f, argb:cc2f343f, argb:cc2f343f
!rofi.color-normal: argb:002f343f, #d8dee8, argb:002f343f, argb:002f343f, #bf616a
!rofi.color-active: #2f343f, #b48ead, #2f343f, #2f343f, #93e5cc
!rofi.color-urgent: #2f343f, #ebcb8b, #2f343f, #2f343f, #ebcb8b


